:root{--primary-color:#F6B95D;--primary-color-10:hsl(34, 81%, 59%, 0.2);--heading:#010000;--text:#4E5255;--background-primary:#FCF9F3;--background-secondary:#fff;--border-radius:4px}*,*::before,*::after{box-sizing:border-box;margin:0}html{font-size:62.5%;-webkit-text-size-adjust:none;text-size-adjust:none}html:focus-within{scroll-behavior:smooth}body{background-color:var(--background-primary);color:var(--text);font-family:-apple-system,BlinkMacSystemFont,segoe ui,Roboto,Oxygen,Ubuntu,Cantarell,open sans,helvetica neue,sans-serif;font-size:1.8rem;font-weight:300;line-height:1.5;text-rendering:optimizeSpeed}h1,h2,h3,blockquote{color:var(--heading);font-weight:600;line-height:1.2;letter-spacing:-.2px}h1{font-size:4.8rem;margin-bottom:1.6rem}h2{font-size:3.2rem;margin-bottom:1.6rem}h3{font-size:2rem;line-height:1.5;margin:2.4rem 0}p{margin-bottom:2.4rem}b,strong{font-weight:500}blockquote{font-size:4rem;margin:2.4rem 0}figcaption{font-size:1.4rem}cite{font-size:1.6rem;font-weight:300;opacity:.8}figcaption{font-weight:300;font-style:italic}a:not([class]){color:var(--text);text-decoration-skip-ink:auto}a:hover{color:var(--primary-color)}ul{margin:1.6rem 0}li{margin-bottom:1.6rem}img,picture{max-width:100%;height:auto;display:block}hr{background-color:var(--text);border:none;opacity:.1;height:1px;margin:4rem auto;width:100%}p~.btn{margin-top:1.6rem}.btn{border-radius:var(--border-radius);font-weight:600;font-size:1.6rem;line-height:1.4;text-decoration:none;display:inline-flex;justify-content:center;align-items:center;max-width:max-content;padding:1.4rem 4.8rem}.btn-primary{background-color:var(--heading);color:#fff}.btn-primary:hover{background-color:var(--primary-color);color:var(--heading);transition:ease-in-out .2s}.btn-secondary{background-color:transparent;border:2px solid var(--heading);border-radius:var(--border-radius);color:var(--heading)}.btn-secondary:hover{background-color:var(--primary-color-10);color:var(--heading);transition:ease-in-out .2s}.main-header{display:flex;justify-content:center;margin:0 2.4rem}.navigation{display:flex;flex-direction:row;justify-content:space-between;align-items:flex-start;padding:1.6rem 0;max-width:120rem;width:100%}.navigation a{text-decoration:none}.nav-list{text-align:center;list-style-type:none;display:flex;flex-direction:column;justify-content:center;align-items:center;gap:4rem;padding-left:0;margin:12rem 0 4rem}.nav-item a:not(.btn){font-weight:600;padding:1.4rem 2.4rem}.nav-item a:not(.btn):hover{background-color:var(--primary-color-10)}.nav-item a{color:#000}.nav-item:last-child a{color:#fff}.nav-item:last-of-type{color:#fff;margin-top:1.6rem}button{background:0 0;border:none;cursor:pointer;display:block;margin-top:.8rem}button svg{color:#000;width:4.8rem;height:auto}.nav-list[hidden]{display:none}@media screen and (min-width:960px){.navigation{flex-direction:row;justify-content:space-between;align-items:center}.navigation button{display:none}.nav-list[hidden]{display:flex;flex-direction:row;gap:2.4rem}.nav-list{margin:0}.nav-item:last-of-type{margin-top:0}}main{display:flex;flex-direction:column;align-items:center;margin:auto 2.4rem}.hero,.annonce,.services,.contact{margin-bottom:24rem}.page,.article,.list{margin-bottom:12rem}.hero{margin-top:8rem}.hero-info h1{font-size:6.4rem;font-weight:700}.yellow{color:var(--primary-color)}.hero,.services{display:flex;flex-direction:column;gap:24rem;align-items:center;justify-content:center;max-width:120rem}.image-text-block{display:flex;flex-direction:column;flex-wrap:nowrap;align-items:center;gap:4rem}.services img{width:100%;height:100%;max-width:545px;max-height:auto}.hero-info,.info{max-width:54.5rem}@media screen and (min-width:1200px){.image-text-block{flex-direction:row;width:100%;gap:12rem}.reverse{flex-direction:row-reverse}}.contact{text-align:center;display:flex;flex-direction:column;place-items:center}.page,.article{display:grid;grid-template-columns:1fr minmax(100px,64ch)1fr;max-width:120rem}.page>*:not(img),.article>*:not(img){gr